<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="struts-cikp-user" extends="struts-default" namespace="/user">
        <action name="DefineActor" class="defineActorAction">
            <result name="input">/jsp/cikp/user/user_profile.jsp</result>
            <result>/jsp/cikp/user/user_interface.jsp</result>
        </action>
        <action name="ItemsPicker" class="itemsPickerAction">
            <result>/jsp/cikp/user/items_picker.jsp</result>
        </action>
        <action name="*" class="userSupport">
            <result>/jsp/cikp/user/{1}.jsp</result>
        </action>
	</package>
</struts>