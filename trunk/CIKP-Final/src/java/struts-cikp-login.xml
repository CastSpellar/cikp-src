<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="struts-cikp-login" extends="struts-default" namespace="/login">
        <action name="Login" class="loginAction">
            <result name="input">/jsp/cikp/login/login.jsp</result>
            <result>/jsp/cikp/login/success.jsp</result>
        </action>
        <action name="*" class="loginSupport">
            <result>/jsp/cikp/login/{1}.jsp</result>
        </action>
	</package>
</struts>
