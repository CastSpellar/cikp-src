<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="struts-cikp-organization" extends="struts-default" namespace="/organization">
        <action name="DefineOrganization" class="defineOrganizationAction">
            <result name="input">/jsp/cikp/organization/organization_profile.jsp</result>
            <result>/jsp/cikp/user/user_interface.jsp</result>
        </action>
        <action name="*" class="organizationSupport">
            <result>/jsp/cikp/organization/{1}.jsp</result>
        </action>
	</package>
</struts>