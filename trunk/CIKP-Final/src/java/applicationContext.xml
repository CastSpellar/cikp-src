        class="org.fct.unl.pt.cikp.actions.comunity.of.practice.DefineComunityOfPractice" />
    <bean
        id="subscribeComunityOfPracticeAction"
        class="org.fct.unl.pt.cikp.actions.comunity.of.practice.SubscribeComunityOfPractice" />
    <bean
        id="comunityOfPracticeSupport"
        class="org.fct.unl.pt.cikp.actions.comunity.of.practice.ComunityOfPracticeSupport" />

