<!DOCTYPE beans PUBLIC 
	"-//SPRING//DTD BEAN//EN" 
	"http://www.springframework.org/dtd/spring-beans.dtd">
	
<beans>
	<!-- Example of SAF2 action instantiated by Spring -->
    <bean
        id="loginAction"
        class="org.fct.unl.pt.cikp.actions.login.Login" />
    <bean
        id="loginSupport"
        class="org.fct.unl.pt.cikp.actions.login.LoginSuport" />
    <bean
        id="registerAction"
        class="org.fct.unl.pt.cikp.actions.register.Register" />
    <bean
        id="registerSupport"
        class="org.fct.unl.pt.cikp.actions.register.RegisterSuport" />
    <bean
