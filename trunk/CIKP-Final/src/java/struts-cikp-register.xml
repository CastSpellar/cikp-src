<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="struts-cikp-register" extends="struts-default" namespace="/register">
        <action name="Register" class="registerAction">
            <result name="input">/jsp/cikp/register/register.jsp</result>
            <result name="organizationsuccess">/jsp/cikp/register/organizationsuccess.jsp</result>
            <result name="usersuccess">/jsp/cikp/register/usersuccess.jsp</result>
        </action>
        <action name="*" class="registerSupport">
            <result>/jsp/cikp/register/{1}.jsp</result>
        </action>
	</package>
</struts>
