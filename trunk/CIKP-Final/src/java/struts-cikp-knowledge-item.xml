<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="struts-cikp-knowledge-item" extends="struts-default" namespace="/knowledgeitem">
        <action name="DefineKnowledgeItem" class="defineKnowledgeItemAction">
            <result name="input">/jsp/cikp/knowledgeitem/knowledge_item_profile.jsp</result>
            <result type="redirect">/user/user_interface.action</result>
        </action>
        <action name="SubscribeKnowledgeItem" class="subscribeKnowledgeItemAction">
            <result name="input">/jsp/cikp/subscribe/subscribe.jsp</result>
            <result type="redirect">/user/user_interface.action</result>
        </action>
        <action name="*" class="knowledgeItemSupport">
            <result>/jsp/cikp/knowledgeitem/{1}.jsp</result>
        </action>
	</package>
</struts>
